   },
   "outputs": [],
   "source": [
    "def numerical_pdf(V,K,h):\n",
    "    num_pdf = (V(K+h)-2*V(K)+V(K-h))/h**2\n",
    "    return num_pdf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def lognormal_pdf(F,vol):\n",
    "    lognormal_pdf = lognorm.pdf(F,vol)\n",
    "    return lognormal_pdf"
   ]
